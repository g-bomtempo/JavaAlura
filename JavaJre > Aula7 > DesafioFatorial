
public class DesafioFatorial {

	public static void main(String[] args) {
		int total = 1;
		for (int linha = 0; linha <= 10; linha++) {
			for (int fatorial = 1; linha <= 10; fatorial++) {
				if (fatorial == linha + 1) {
					total = total * fatorial;
					System.out.println("O fatorial de " + linha + " Ã© " + total);
					break;
				}
			}
		}

	}

}
